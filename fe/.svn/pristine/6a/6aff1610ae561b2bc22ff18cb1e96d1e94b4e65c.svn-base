.s_header-nav{
    display: inline-block;
    margin-left: -30px;
    list-style: none;
    li{
        float: left;
        height: 57px;
        line-height: 60px;
        width: 66px;
        text-align: center;
        color: #eee;
        font-size: 15px;
        cursor: pointer;
        border-bottom: 3px solid transparent;
        margin-right: 32px;
    }

    li.selected{
        color: #ccc;
        border-bottom: 3px solid rgb(247,220,86);
    }

    a{
        display: block;
        color: #fff;
        font-weight: bold;
        font-size: 14px;
        text-decoration: none;
    }

    

}
