{%extends file="../layout_admin.tpl"%}
{%block name="title"%}
BDE-数据列表
{%/block%}
{%block name="static-resource"%}
    {%require name="bde:static/css/biu/biudatalist.less"%}
{%/block%}

{%block name="header-nav"%}
    {%widget name="bde:widget/headerNav/headerNav.tpl"
            datalist=' selected'
    %}
{%/block%}
{%block name="content"%}
    <div class="page-wrapper">
        {%widget 
            name="bde:widget/dataSelectFilter/dataSelectFilter.tpl"
            priviledge='on'
            %}
    </div>
    
{%script%}
require(['/dataSelectFilter/dataSelectFilter', 'jquery'], function(dataSelect){
    dataSelect.build('/bde/api/datalist', {
        priviledge: true
    });
});
{%/script%}
{%/block%}
