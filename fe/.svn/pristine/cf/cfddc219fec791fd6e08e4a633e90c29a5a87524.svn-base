{%extends file="../layout_admin.tpl"%}
{%block name="title"%}
BDE-ACL-修改ACL
{%/block%}
{%block name="static-resource"%}
    {%require name="bde:static/css/admin/modifyacl.less"%}
{%/block%}

{%block name="header-nav"%}
    {%widget name="bde:widget/headerNav/headerNav.tpl"
            mode='admin'
            acl=' selected'
    %}
{%/block%}
{%block name="content"%}
<div class="page-wrapper">
    {%widget 
        name="bde:widget/subNav/subAclNav.tpl"
        acllist="selected"

    %}
   <div class="form-container">
        {%widget name="bde:widget/aclForm/aclForm.tpl"%}
    </div>
</div>
{%require name="bde:page/admin/createacl.tpl"%}

{%script%}
    require(['/aclForm/aclForm'], function(form){
            form.init({
                mode: 'update',
                extraInfo: ''
            });
    });
{%/script%}
{%/block%}
